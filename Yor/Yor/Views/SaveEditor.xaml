<adonisControls:AdonisWindow
    x:Class="Yor.Views.SaveEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Yor"
    xmlns:adonisControls="clr-namespace:AdonisUI.Controls;assembly=AdonisUI"
    WindowStartupLocation="CenterScreen"
    Height="700"
    Width="1100"
    MinHeight="700"
    MinWidth="1100"
    Background="#21252B"
    Icon="/Assets/logo_0.png" Loaded="AdonisWindow_Loaded">

    <adonisControls:AdonisWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Resources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </adonisControls:AdonisWindow.Resources>
    <TabControl Background="{StaticResource GridToolsBackground}">
        <TabItem Header="Information">

        </TabItem>
        <TabItem Header="Json">
            <TabControl x:Name="JSONContent" Background="{StaticResource GridToolsBackground}" TabStripPlacement="Left">
                
            </TabControl>
        </TabItem>
        <TabItem Header="XML">
            <TabControl x:Name="XMLContent" Background="{StaticResource GridToolsBackground}">

            </TabControl>
        </TabItem>
    </TabControl>
</adonisControls:AdonisWindow>
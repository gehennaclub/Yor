    xmlns:local="clr-namespace:Yor"
    xmlns:adonisControls="clr-namespace:AdonisUI.Controls;assembly=AdonisUI"
    WindowStartupLocation="CenterScreen"
    Height="700"
    Width="1100"
    MinHeight="700"
    MinWidth="1100"
    Background="#21252B"

    <adonisControls:AdonisWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Resources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </adonisControls:AdonisWindow.Resources>
    <TabControl Background="{StaticResource GridToolsBackground}" Loaded="TabControl_Loaded">
        <TabItem Header="Information">
            
        </TabItem>
        <TabItem Header="Json">

        </TabItem>
        <TabItem Header="XML">

        </TabItem>
    </TabControl>
</adonisControls:AdonisWindow>
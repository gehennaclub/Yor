<adonisControls:AdonisWindow x:Class="Yor.Views.About"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:local="clr-namespace:Yor"
                             xmlns:adonisControls="clr-namespace:AdonisUI.Controls;assembly=AdonisUI"
                             WindowStartupLocation="CenterScreen" ResizeMode="NoResize"
                             IconVisibility="Collapsed" Width="500" Height="300">
    <adonisControls:AdonisWindow.Style>
        <Style TargetType="adonisControls:AdonisWindow" BasedOn="{StaticResource {x:Type adonisControls:AdonisWindow}}" >
            <Setter Property="Title" Value="About"/>
        </Style>
    </adonisControls:AdonisWindow.Style>
    <StackPanel Margin="30 10">
        <Label Content="Version:"/>
        <TextBlock x:Name="version"/>
        <Label Content=""/>
        <Label Content="Description:"/>
        <TextBlock x:Name="desc"/>
        <Label Content=""/>
        <Label Content=""/>
        <Label Content="Author:"/>
        <TextBlock x:Name="author"/>
        <Label Content=""/>
        <Label Content=""/>
        <Label Content="Yor:"/>
        <TextBlock x:Name="repository"/>
    </StackPanel>
</adonisControls:AdonisWindow>